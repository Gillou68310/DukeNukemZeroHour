    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "GDB Client(Win32)",
            "executable": "build/dukenukemzerohour.elf",
            "target": ":4600",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "${workspaceRoot}\\tools\\debugger\\win32\\gdb-multiarch-9.2\\bin\\gdb-multiarch.exe",
